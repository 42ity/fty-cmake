@PACKAGE_INIT@

if(EXISTS "${CMAKE_CURRENT_LIST_DIR}/CMakeFiles")
    set(FTY_CMAKE_CMAKE_DIR "@CMAKE_CURRENT_SOURCE_DIR@")
else()
    set(FTY_CMAKE_CMAKE_DIR "@PACKAGE_CMAKE_INSTALL_DIR@/@target@")
endif()

set(FTY_CMAKE_CMAKE_DIR ${RAVEN_CMAKE_DIR} CACHE INTERNAL "" FORCE)

include(${FTY_CMAKE_CMAKE_DIR}/scripts/fty-cmake.cmake)

if (NOT FTY_CMAKE_CMAKE_FOUND)
  set(FTY_CMAKE_CMAKE_FOUND ON CACHE INTERNAL "" FORCE)
endif ()
